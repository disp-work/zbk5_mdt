{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.hcc.bk5.mdt",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZBK5_AUP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"valueHelp": {
				"uri": "",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.hcc.bk5.mdt.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.52.26",
			"libs": {
				"sap.collaboration": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"config": {
			"fullWidth": true
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "com.hcc.bk5.mdt.view",
				"controlId": "FclMdt",
				"transition": "slide",
				"async": true,
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "TargetMasterMdt",
					"pattern": "",
					"layout": "OneColumn",
					"target": [
						"TargetMasterMdt"
					]
				},
				{
					"name": "TargetDetailMdt",
					"pattern": "DetailMdt/:MdtId:/",
					"layout": "TwoColumnsMidExpanded",
					"target": [
						"TargetMasterMdt",
						"TargetDetailMdt"
					]
				}
			],
			"targets": {
				"TargetMasterMdt": {
					"viewName": "MasterMdt",
					"controlAggregation": "beginColumnPages"
				},
				"TargetDetailMdt": {
					"viewName": "DetailMdt",
					"controlAggregation": "midColumnPages"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.hcc.bk5.mdt.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zbk5_mdt/webapp",
		"_version": "1.1.0"
	}
}